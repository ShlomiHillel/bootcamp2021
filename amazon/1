<script type="text/javascript">

P.when('A', 'a-truncate', 'atf').execute('show-tabular-buybox-side-sheet', function(A, truncate) {
"use strict";
var $ = A.$;
var ue = window.ue;

$("#tabular-buybox-side-sheet-content a").attr("target","_blank");
function incrementCounter(counterName) {
if (ue && ue.count) {
ue.count(counterName, (ue.count(counterName) || 0) + 1);
}
}
function getElementIdFromTruncateObject(truncateObject) {
return truncateObject._$element[0] ? truncateObject._$element[0].id : null;
}
function logInteractionCount(baseInteractionType, interactionContextName) {
incrementCounter(baseInteractionType);
if (interactionContextName) {
incrementCounter(baseInteractionType+':'+interactionContextName);
}
}
$("#tabular-buybox #sellerProfileTriggerId").ready(function(e){

window.tabularBuyBoxClickLogger = function () {
incrementCounter("BuyBoxTabularLayoutDesktopLinkClicked");
}
$("#tabular-buybox #sellerProfileTriggerId").attr("onclick", "tabularBuyBoxClickLogger()");
});
incrementCounter("BuyBoxTabularLayoutDesktopDisplayed");

A.on('a:accordion:buybox-accordion:newAccordionRow:select', function () {
$(".tabular-buybox-content").each(function() {
truncate.get(this).update();
});
});





A.on('a:truncate:tabular-buybox-truncate:updated', function(data) {
if (data.truncateInstance.getIfTextFits()) {
A.hide("#tabular-buybox-show-more");
if (ue && ue.count) {
logInteractionCount('DesktopTabularContentExpanded:T1', getElementIdFromTruncateObject(data.truncateInstance));
}
} else {
A.show("#tabular-buybox-show-more");
logInteractionCount('DesktopTabularContentTruncated:T1', getElementIdFromTruncateObject(data.truncateInstance));
}
});


A.declarative('show-tabular-buybox-side-sheet', 'click', function(event){
showSideSheet();
logInteractionCount('DesktopTabularSideSheetOpened', event.data.contextualName);
});
A.declarative('close-tabular-buybox-side-sheet', 'click', function(event){
closeSideSheet();
logInteractionCount('DesktopTabularSideSheetClosed', event.data.contextualName);
});
function showSideSheet() {
A.show('#tabular-buybox-background');
animateOpenSideSheet();
}
function closeSideSheet() {
animateCloseSideSheet();
A.hide('#tabular-buybox-background');
}
function animateOpenSideSheet() {
$("#tabular-buybox-side-sheet-content").show(function (){
$(this).addClass("tabular-buybox-side-sheet-content-display");
});
}
function animateCloseSideSheet(){
$("#tabular-buybox-side-sheet-content").show(function (){
$(this).removeClass("tabular-buybox-side-sheet-content-display");
});
}
});
</script>

























